# Cloudflare D1 Database Configuration
# Copy this file to .env and fill in your actual values

# Local development database path (for wrangler dev)
D1_DATABASE_PATH=".wrangler/state/v3/d1/miniflare-D1DatabaseObject/airports-db.sqlite"

# Cloudflare API Token (for deployment)
CLOUDFLARE_API_TOKEN="your-api-token-here"

# Database IDs (will be generated when creating D1 databases)
D1_DATABASE_ID_DEV=""
D1_DATABASE_ID_PROD=""

# Wikipedia API settings
WIKIPEDIA_API_BASE="https://en.wikipedia.org/api/rest_v1"